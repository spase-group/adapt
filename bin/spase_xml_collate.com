
TARGET_DIRECTORY=$1

FILE_NAME_INPUT=$2

RESOURCE_ID=`grep '<ResourceID>' $FILE_NAME_INPUT | awk 'BEGIN { FS="[<>]" } ; { print $3 }' | sed 's/spase:\/\///'`

FILE_NAME_OUTPUT=`basename $RESOURCE_ID.xml`

DIRECTORY_OUTPUT=`dirname $RESOURCE_ID`

mkdir -p $TARGET_DIRECTORY/$DIRECTORY_OUTPUT

cp -p $FILE_NAME_INPUT $TARGET_DIRECTORY/$DIRECTORY_OUTPUT/$FILE_NAME_OUTPUT

find $TARGET_DIRECTORY/$DIRECTORY_OUTPUT/$FILE_NAME_OUTPUT

